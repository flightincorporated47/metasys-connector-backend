connector:
  name: central-plant-pilot
  environment: dev
  organization_id: ORG_REPLACE_ME
metasys:
  host: https://YOUR-METASYS-SERVER
  api_base: /api/v6
  auth:
    mode: basic
    username: YOUR_USERNAME
    password_env: METASYS_PASSWORD
  verify_tls: true
  assets:
  - asset_id: CHW_PUMP_1
    name: CHW Pump 1
    points:
    - point_id: CHW_P1_STATUS
      name: Run Status
      data_type: bool
      tier: 1
      source_ref: metasys:ref:REPLACE_ME
    - point_id: CHW_P1_SPEED
      name: Speed
      data_type: float
      tier: 1
      source_ref: metasys:ref:REPLACE_ME
      deadband: 0.2
      min_publish_seconds: 5
    - point_id: CHW_P1_DP
      name: Differential Pressure
      data_type: float
      tier: 2
      source_ref: metasys:ref:REPLACE_ME
      deadband: 0.05
      min_publish_seconds: 5
polling:
  tick_seconds: 0.25
  flush_interval_seconds: 5
  max_points_per_batch: 200
  out_dir: out
deltas:
  enabled: true
ingest:
  endpoint_url: https://REPLACE_ME/ingest
  mode: file
queue:
  enabled: true
  path: ./.queue/metasys-connector
  max_disk_mb: 500
  drop_policy: oldest
